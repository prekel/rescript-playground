{
  "name": "rescript-playground",
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "spec",
      "subdirs": true,
      "type": "dev"
    }
